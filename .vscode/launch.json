{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug polyaie-opt Pipeline",
            "type": "cppdbg",
            "request": "launch",
            "program": "/home/douliyang/large/mlir-workspace/micro-polyaie/build/bin/polyaie-opt",
            "args": [
                "/home/douliyang/large/mlir-workspace/micro-polyaie/tutorial/gemm.mlir",
                "-polyaie-pipeline=top-func-name=gemm return-all-arg=false tile-size=32 vec-size=1 algorithm=simulated-annealing enable-create-interface=false enable-link-extern-kernel=true object-file=kernel.o gen-extern-kernel=false",
                "-debug"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "VITIS_DIR",
                    "value": "/tools/Xilinx/Vitis/2020.1"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
